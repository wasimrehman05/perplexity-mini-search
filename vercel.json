{
    "version": 2,
    "builds": [
        {
            "src": "api/app.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/query",
            "methods": [
                "POST"
            ],
            "dest": "/api/app.js"
        },
        {
            "src": "/(.*)",
            "dest": "/public/index.html"
        }
    ]
}